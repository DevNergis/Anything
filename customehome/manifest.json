{
  "manifest_version": 3,
  "name": "customHome",
  "description": "Make Your Custom Home",
  "version": "1.0",
  "action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "bookmarks",
    "topSites",
    "activeTab",
    "tabs",
    "storage",
    "history"
  ], 
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}